{
  "name": "webamp-torrent",
  "version": "1.0.0",
  "description": "Webamp + torrent",
  "scripts": {
    "copy-public": "mkdir -p dist && cp -r public/* dist && echo public copied",
    "start": "npm run copy-public && parcel src/index.html",
    "build": "npm run copy-public && parcel build --no-source-maps src/index.html"
  },
  "keywords": [
    "webamp",
    "webtorrent",
    "torrent",
    "music",
    "player"
  ],
  "author": "Alberto Miranda",
  "license": "ISC",
  "dependencies": {
    "drag-drop": "^7.2.0",
    "mime-types": "^2.1.33",
    "multiformats": "^9.4.10",
    "webamp": "^1.5.0",
    "webtorrent": "^1.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "parcel": "^2.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
